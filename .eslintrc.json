{
  "extends": "aqua/node",
  "rules": {
    "indent": [
      "warn",
      2
    ],
    "quotes": [
      "error",
      "single",
      {
        "allowTemplateLiterals": false,
        "avoidEscape": true
      }
    ],
    "func-names": [
      "error",
      "as-needed"
    ],
    "strict": [
      "error",
      "global"
    ],
    "eqeqeq": 0,
    "guard-for-in": 0,
    "no-shadow": [
      "error",
      {
        "hoist": "functions"
      }
    ],
    "prefer-destructuring": 0
  }
}