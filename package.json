{
  "name": "magibot",
  "version": "3.2.0",
  "description": "MagiBot - a Discord bot to build communities",
  "main": "dist/index.js",
  "engines": {
    "node": "20.10.0"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "@discordjs/builders": "1.7.0",
    "@discordjs/rest": "2.2.0",
    "@discordjs/voice": "0.16.1",
    "axios": "1.6.3",
    "blapi": "3.1.2",
    "bufferutil": "4.0.8",
    "discord-api-types": "0.37.67",
    "discord.js": "14.14.1",
    "dotenv": "16.3.1",
    "fast-folder-size": "2.2.0",
    "ffmpeg-static": "5.2.0",
    "ffprobe": "1.1.2",
    "forever": "4.0.3",
    "libsodium-wrappers": "0.7.13",
    "mongoose": "7.6.7",
    "opusscript": "0.1.1",
    "statcord.js": "3.4.3",
    "tweetnacl": "1.0.3",
    "utf-8-validate": "6.0.3"
  },
  "optionalDependencies": {
    "@discordjs/opus": "0.9.0",
    "sodium": "3.0.2",
    "zlib-sync": "0.1.9"
  },
  "devDependencies": {
    "@types/centra": "2.2.3",
    "@types/ffprobe": "1.1.4",
    "@types/node": "20.10.6",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint": "8.55.0",
    "eslint-config-airbnb-base": "15.0.0",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "typescript": "5.3.3"
  },
  "scripts": {
    "install-dev": "npm i --no-optional",
    "dev": "npm run build && node dist/index.js",
    "lint": "eslint \"src/**/*\"",
    "lint:fix": "eslint \"src/**/*\" --fix",
    "build": "rimraf ./dist && tsc",
    "start": "node dist/index.js",
    "start:forever": "forever start dist/index.js",
    "stop": "forever stop dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/T0TProduction/MagiBot.git"
  },
  "author": "Sebastian Di Luzio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/T0TProduction/MagiBot/issues"
  },
  "homepage": "https://github.com/T0TProduction/MagiBot#readme",
  "heroku-run-build-script": true
}
