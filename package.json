{
  "name": "magibot",
  "version": "3.2.0",
  "description": "MagiBot - a Discord bot to build communities",
  "main": "dist/index.js",
  "engines": {
    "node": "16.x"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "@discordjs/builders": "1.3.0",
    "@discordjs/rest": "1.3.0",
    "@discordjs/voice": "0.13.0",
    "axios": "^1.1.3",
    "blapi": "3.0.0",
    "bufferutil": "^4.0.7",
    "discord-api-types": "0.37.12",
    "discord.js": "13.12.0",
    "dotenv": "16.0.3",
    "fast-folder-size": "1.7.1",
    "ffmpeg-static": "5.1.0",
    "ffprobe": "1.1.2",
    "ffprobe-static": "3.1.0",
    "forever": "4.0.3",
    "libsodium-wrappers": "^0.7.10",
    "mongoose": "6.7.2",
    "opusscript": "0.0.8",
    "statcord.js": "3.4.3",
    "tweetnacl": "^1.0.3",
    "utf-8-validate": "^5.0.10"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.9.0",
    "sodium": "^3.0.2",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/centra": "2.2.0",
    "@types/ffprobe": "1.1.3",
    "@types/ffprobe-static": "2.0.1",
    "@types/node": "16.18.3",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.28.0",
    "eslint-config-airbnb-base": "15.0.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.9.3"
  },
  "scripts": {
    "install-dev": "npm i --no-optional",
    "dev": "npm run build && node dist/index.js",
    "lint": "eslint \"src/**/*\"",
    "lint:fix": "eslint \"src/**/*\" --fix",
    "build": "rimraf ./dist && tsc",
    "start": "node dist/index.js",
    "start:forever": "forever start dist/index.js",
    "stop": "forever stop dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/T0TProduction/MagiBot.git"
  },
  "author": "Sebastian Di Luzio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/T0TProduction/MagiBot/issues"
  },
  "homepage": "https://github.com/T0TProduction/MagiBot#readme",
  "heroku-run-build-script": true
}
